{
  "dependencies":{
    "qili-app":"^1.0.0",
    "react": "16.7.0",
    "react-dom": "16.7.0",
    "graphql": "^14.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-module-resolver": "^2.7.1",
    "babel-plugin-relay": "^1.3.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.6.0",
    "babel-plugin-transform-object-rest-spread": "^6.6.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "cordova": "5.4.0",
    "css-loader": "^0.28.4",
    "html-webpack-inline-source-plugin": "^0.0.9",
	  "html-webpack-plugin": "^2.30.1",
    "imports-loader": "^0.7.1",
    "less": "2.7.2",
    "less-loader": "^4.0.5",
    "relay-compiler": "^5.0.0",
    "source-map-loader": "^0.2.1",
    "style-loader": "^0.18.2",
    "webpack": "^3.4.1",
    "webpack-dev-server": "^2.6.1",
    "webpack-node-externals": "^1.7.2"
  },  
  "resolutions": {
    "*/react": "16.7.0",
    "*/react-dom": "16.7.0",
    "*/graphql": "^14.0.0",
    "*/minimatch": "^3.0.0",
    "*/recompose": "^0.27.0"
  },
  "scripts": {
    "int":"qili init",
    "build": "webpack",
    "dev": "yarn relay && yarn persist && webpack-dev-server --env=dev",
    "cordova": "cordova create cordova --link-to=dist && cd cordova && cp www/phone.xml config.xml && cordova platform add android && cordova build android",
    "relay": "relay-compiler --src ./src --schema ./schema.graphql",
    "persist": "qili persist src cloud",
    "cloud": "webpack --env=cloud",
	"sync":"qili publish"
  },
  "config":{
      "devPort":9083,
	  "service":"https://api.qili2.com/1/graphql",
	  "appId":""
  },
  "keywords": ["qili"],
  "engines": {
    "node": ">= 8"
  }
}
