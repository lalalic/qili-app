<?xml version='1.0' encoding='utf-8'?>
<widget id="lalalic.qili" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>qili</name>
    <description>
        qili administration
    </description>
    <author email="lalalic@139.com">
        lalalic
    </author>
    <content src="cordova.html" />
    <plugin name="cordova-plugin-whitelist"/>
    <plugin name="cordova-plugin-file" />
    <plugin name="Cordova-sqlite-storage" />
    <plugin name="cordova-plugin-file-transfer" />
    <plugin name="cordova-plugin-camera" />
    <plugin name="cordova-plugin-wechat">
        <variable name="APP_ID" value="xxx" />
    </plugin>
    <plugin name="cordova-plugin-splashscreen"/>
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <icon src="www/images/icon.svg" />
</widget>
