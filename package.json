{
  "name": "qili-app",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "description": "qili dashboard and application framework",
  "version": "1.2.7",
  "homepage": "http://app.qili2.com",
  "author": {
    "name": "Raymond Li",
    "email": "lalalic@139.com"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-jest": "^18.0.0",
    "babel-loader": "^6.3.2",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-module-resolver": "^2.7.1",
    "babel-plugin-relay": "^1.3.0",
    "babel-plugin-transform-class-properties": "^6.6.0",
    "babel-plugin-transform-object-rest-spread": "^6.6.5",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.5.0",
    "cordova": "5.4.0",
    "css-loader": "^0.26.1",
    "html-webpack-harddisk-plugin": "^0.1.0",
    "html-webpack-inline-source-plugin": "^0.0.9",
    "html-webpack-plugin": "^2.30.1",
    "imports-loader": "^0.7.1",
    "jasmine-core": "^2.3.4",
    "jest": "^21.2.1",
    "less": "^2.7.2",
    "less-loader": "^2.2.3",
    "raw-loader": "^0.5.1",
    "react-hot-loader": "1.3.1",
    "relay-compiler": "^1.3.0",
    "rollup": "^0.58.0",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-commonjs": "^9.1.0",
    "rollup-plugin-less": "^0.1.4",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-uglify": "^3.0.0",
    "source-map-loader": "^0.1.6",
    "style-loader": "^0.13.1",
    "text-loader": "^0.0.1",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1"
  },
  "scripts": {
    "build": "webpack",
    "dev": "webpack-dev-server --hot --inline --env=dev",
    "test": "jest",
    "cordova": "cordova create cordova --link-to=packages/console/dist && cd cordova && cordova platform add android && cordova build android --release -- --keystore=../lalalic.keystore --alias=lalalic",
    "relay": "relay-compiler --src ./packages --include **/src/**  --schema ./packages/console/schema.graphql",
    "persist": "qili-persist packages ../qili/lib/admin",
    "api": "rollup -c --no-treeshake -o ./packages/qili-app/index.js -f cjs -m inline ./packages/qili-app/src/index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lalalic/qili-app"
  },
  "engines": {
    "node": ">= 6"
  },
  "readme": "README.md"
}
