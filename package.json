  },
  "dependencies": {
    "babel-runtime": "^6.18.0",
    "material-auto-rotating-carousel": "^1.1.0",
    "material-ui": "^0.16.0",
    "minimongo": ">=3.2.13",
    "normalizr": "^2.2.1",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-intl": "^2.1.5",
    "react-motion": "^0.4.5",
    "react-redux": "^4.4.5",
    "react-responsive": "^1.2.5",
    "react-router": "^2.4.1",
    "react-swipeable-views": "^0.7.7",
    "react-tap-event-plugin": "^2.0.0",
    "reactable": "^0.13.2",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-transform-class-properties": "^6.6.0",
    "babel-plugin-transform-object-rest-spread": "^6.6.5",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.2.0",
    "browserify": "^12.0.1",
    "fakexmlhttprequest": "0.0.1",
    "jasmine-core": "^2.3.4",
    "karma": "^0.13.15",
    "karma-browserify": "^4.4.2",
    "karma-chrome-launcher": "^0.2.1",
    "karma-jasmine": "^0.3.6",
    "node-lessify": "0.0.10",
    "react-addons-test-utils": "^15.1.0",
    "react-stub-context": "^0.7.0",
    "request": "^2.67.0",
    "restmock": "0.1.x",
    "svg-to-png": "^3.1.2",
    "through2": "^2.0.0",
    "uglifyify": "^3.0.1",
    "watchify": "^3.6.1"
  },
  "browserify": {
    "transform": [
      [
        "node-lessify",
        {
          "plugins": []
        }
      ]
    ]
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ],
    "plugins": [
      "babel-plugin-add-module-exports",
      "babel-plugin-transform-object-rest-spread",
      "babel-plugin-transform-class-properties",
      "babel-plugin-transform-runtime"
    ]
  },
  "scripts": {
    "build": "babel src -d lib --source-maps inline",
    "build-superdaddy": "babel src -d ../superdaddy/node_modules/qili-app/lib --source-maps inline",
    "build-we-travel": "babel src -d ../we-travel/node_modules/qili-app/lib --source-maps inline",
    "merge": "NODE_ENV=production browserify -g uglifyify ./lib/main.js -o dist/index.js -i jquery && node scripts/allin1.js",
    "merge.debug": "browserify -g uglifyify ./lib/main.js -o dist/test.js -i jquery",
    "karma": "karma start",
    "test": "restmock",
    "test.mock": "watchify -v -d ./.test.mock.js -o dist/test.js",
    "test.mongo": "NODE_ENV=production watchify -v -d ./.test.mongo.js -o dist/test.js",
    "cordova": "cordova create cordova --link-to=dist && cd cordova && cordova platform add android && cordova build android -d"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "engines": {
    "node": ">= 0.4.4"
  },
  "main": "lib",
  "readme": "README.md"
}
