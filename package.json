  "browserify": {
    "transform": [
      [
        "node-lessify",
        {
          "plugins": []
        }
      ]
    ]
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ],
    "plugins": [
      "babel-plugin-add-module-exports",
      "babel-plugin-transform-object-rest-spread",
      "babel-plugin-transform-class-properties"
    ]
  },
  "scripts": {
    "build": "babel src -d lib --source-maps inline",
	"build1": "babel src -d ../we-travel/node_modules/qili-app/lib --source-maps inline",
    "dist": "browserify -g uglifyify ./lib/main.js -o dist/index.js",
    "merge": "npm run dist && node scripts/allin1.js",
    "karma": "karma start",
    "test": "restmock",
    "test.mock": "watchify -v -d ./.test.mock.js -o dist/test.js",
    "test.mongo": "watchify -v -d ./.test.mongo.js -o dist/test.js",
    "cordova": "cordova create cordova --link-to=dist"
  },
  "repository": {
    "type": "git",
